{
    "map": {
        "items": "items"
    },
    "filename": "package.json"
}
---
{
  "name": "@siocode/not-a-bird",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "chalk": "4",
    "chokidar": "3.6.0",
    "concurrently": "8.2.2",
    "esbuild": "0.20.2",
    "fs-extra": "11.2.0",
    "sass": "1.75.0",
    "typescript": "5.4.5",
    "prettier": "^3.2.5"
  },
  "scripts": {
    "build:contracts": "{{#each items}}npm run -w @not-a-bird/contract-{{kebabCase name}} build{{#unless @last}} && {{/unless}}{{/each}}",
    "build": "npm run -w @not-a-bird/app-game build",
    "serve": "npm run -w @not-a-bird/app-game serve"
  }
}
